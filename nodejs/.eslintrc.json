{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "max-len": ["error", {"code": 130}],
    "quotes": ["warn", "double"],
    "indent": ["warn", 4],
    "brace-style": ["warn", "stroustrup"],
    "valid-jsdoc": ["warn"],
    "require-jsdoc": ["warn"]
  }
}
